{
  "kind": "collectionType",
  "collectionName": "commodities",
  "info": {
    "singularName": "commodity",
    "pluralName": "commodities",
    "displayName": "commodity",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "brand": {
      "type": "enumeration",
      "enum": [
        "RSER",
        "beichenyipin",
        "guanxianchun",
        "yumeng",
        "xianbeiya",
        "aiduoxi"
      ],
      "required": true
    },
    "coverImg": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "characteristic": {
      "type": "text"
    },
    "introduce": {
      "type": "text"
    },
    "context": {
      "type": "customField",
      "options": {
        "output": "HTML",
        "preset": "rich"
      },
      "customField": "plugin::ckeditor.CKEditor"
    },
    "isHot": {
      "type": "boolean"
    },
    "type": {
      "type": "enumeration",
      "enum": [
        "dog",
        "cat"
      ],
      "default": "cat",
      "required": true
    },
    "productRange": {
      "type": "string",
      "required": true,
      "default": "宠物零食"
    }
  }
}
